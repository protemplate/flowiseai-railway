PORT=3000
DATABASE_PATH=/root/.flowise
APIKEY_PATH=/root/.flowise
SECRETKEY_PATH=/root/.flowise
LOG_PATH=/root/.flowise/logs
BLOB_STORAGE_PATH=/root/.flowise/storage

# Database Configuration for shared PostGIS instance
DATABASE_TYPE=postgres
DATABASE_PORT=5432
DATABASE_HOST=postgis
DATABASE_NAME=flowise
DATABASE_USER=postgres
DATABASE_PASSWORD=postgrespassword
POSTGRES_DB=railway

# CORS Configuration
CORS_ORIGINS=*
IFRAME_ORIGINS=*

# Queue Configuration for local development
MODE=queue
QUEUE_NAME=flowise-queue
REDIS_HOST=redis # Local Redis container, NOT redis.railway.internal
REDIS_PORT=6379
WORKER_CONCURRENCY=10000